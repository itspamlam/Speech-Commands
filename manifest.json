{
    "manifest_version": 2,
    "name": "Goose Says...",
    "version": "1.0",
    "description": "Voice command actions on browser",
    "browser_action": {
        "default_title": "Goose Says...?",
        "default_popup": "start.html"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["goose.js"]
    }
  ]
}